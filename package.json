{
  "name": "@tailwindcss/forms",
  "version": "0.5.8",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tailwindlabs/tailwindcss-forms.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "scripts": {
    "dev": "concurrently \"npm run serve\" \"npm run watch\"",
    "serve": "live-server src",
    "watch": "npm run build -- -w",
    "build": "tailwindcss -o src/output.css",
    "test": "exit 0",
    "release-channel": "node ./scripts/release-channel.js",
    "release-notes": "node ./scripts/release-notes.js",
    "format": "prettier . --write"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0 || >= 3.0.0-alpha.1 || >= 4.0.0-alpha.20"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.6",
    "concurrently": "^5.3.0",
    "live-server": "^1.2.0",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "daisyui": "^4.12.14",
    "express": "^4.21.2",
    "mini-svg-data-uri": "^1.2.3"
  },
  "keywords": [],
  "author": "",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/tailwindlabs/tailwindcss-forms/issues"
  },
  "homepage": "https://github.com/tailwindlabs/tailwindcss-forms#readme",
  "description": ""
}
